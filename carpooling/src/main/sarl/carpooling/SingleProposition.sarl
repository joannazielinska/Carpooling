
package carpooling

import io.sarl.lang.core.Address


class SingleProposition {
	public var who : Address
	public var startLocation : Location
	public var destination : Location
	public var personalData : PersonInformation
	
	public new(who : Address, startLocation : Location, destination : Location, personalData : PersonInformation)
	{
		this.who = who
		this.startLocation = startLocation
		this.destination = destination
		this.personalData = personalData
	}
}
